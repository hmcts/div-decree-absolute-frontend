{
  "name": "divorce-decree-absolute",
  "description": "Divorce Decree Absolute Frontend",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "start": "NODE_PATH=. node server.js",
    "setup": "NODE_PATH=.",
    "dev": "NODE_ENV=dev NODE_PATH=. node-dev server.js",
    "mocks": "NODE_ENV=dev NODE_PATH=. node-dev mocks/index.js",
    "test": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR nyc mocha --exit 'test/unit/**/*.test.js' --timeout 15000",
    "test:all": "yarn lint; yarn test:unit; yarn test:a11y; yarn test:validation; yarn test:functional",
    "test:unit": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR mocha --exit 'test/unit/**/*.js' --timeout 15000",
    "test:smoke": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/functional/codecept.smoke.conf.js --steps",
    "test:coverage": "nyc check-coverage --statements 10 --branches 10 --functions 10 --lines 10",
    "test:a11y": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR mocha --exit test/a11y --reporter spec --recursive --timeout 15000 --reporter mochawesome --reporter-options reportDir=functional-output,reportFilename=a11y",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:functional": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/functional/codecept.conf.js --steps --reporter mocha-multi",
    "test:fullfunctional": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/functional/codecept.conf.js --steps --reporter mocha-multi",
    "test:validation": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR mocha --exit test/w3cjs --timeout 15000",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh",
    "test-crossbrowser-e2e": "NODE_PATH=. NODE_ENV=ci LOG_LEVEL=DEBUG node_modules/.bin/codeceptjs run-multiple  ${BROWSER_GROUP:-'--all'} -c test/functional/saucelabs.conf.js --steps --reporter mocha-multi",
    "lint": "eslint ."
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@hmcts/ctsc-web-chat": "^0.3.11",
    "@hmcts/div-document-express-handler": "^1.1.0",
    "@hmcts/div-idam-express-middleware": "^6.6.2",
    "@hmcts/look-and-feel": "^4.0.8",
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/nodejs-logging": "^3.0.0",
    "@hmcts/one-per-page": "^5.3.0",
    "@hmcts/properties-volume": "^0.0.9",
    "applicationinsights": "1.5.0",
