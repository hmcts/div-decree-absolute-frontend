    "bl": ">=4.0.3",
    "axios": ">=0.21.1",
    "ssri": ">=6.0.2 <7.0.0 || >=8.0.1",
    "netmask": ">=2.0.1",
    "hosted-git-info": "^3.0.8",
    "url-parse": "^1.5.0",
    "postcss": "^8.2.10",
    "trim-newlines": "^4.0.1",
    "redis": "^3.1.1",
    "glob-parent": "^5.1.2"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-directory": "./coverage/.nyc_output",
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test/**/*",
      "mocks/**/*"
    ]
  }
}
