    "cookie-parser": "^1.4.4",
    "cookies": "^0.7.3",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "glob": "^7.1.2",
    "govuk-frontend": "^3.0.0",
    "handlebars": ">=4.7.7",
    "helmet": "^3.21.2",
    "ioredis": "^3.2.2",
    "joi": "^13.7.0",
    "js-yaml": "^3.13.0",
    "jsoneditor": "^5.24.6",
